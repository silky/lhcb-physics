<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension//en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
  <argument name="program"><text>gaudirun.py</text></argument>
  <argument name="args"><set>
    <text>$BRUNELOPTS/COLLISION10-Beam3500GeV-VeloClosed-MagOff.py</text>
    <text>$APPCONFIGOPTS/Brunel/MonitorExpress.py</text>
    <text>--option</text>
    <text>from Brunel.Configuration import *; Brunel().EvtMax=1000;Brunel().Monitors=["SC","FPE"]; Brunel().OutputType="DST"; Brunel().Persistency="POOL"</text>
  </set></argument>
  <argument name="reference"><text>../refs/brunel2010magoff.ref</text></argument>
  <argument name="validator"><text>
from Brunel.QMTest.BrunelExclusions import preprocessor as BrunelPreprocessor
preprocessor = BrunelPreprocessor + \
  LineSkipper(["EventPersistenc...   INFO Added successfully Conversion service:"]) + \
  LineSkipper(["FileRecordPersi...   INFO Added successfully Conversion service:"]) + \
  LineSkipper(["FileRecordCnvSvc     INFO File sharing enabled. Do not retire files"]) + \
  LineSkipper(["DstWriter            INFO Data source: EventDataSvc output: DATAFILE='PFN:"]) + \
  LineSkipper(["FSROutputStream...   INFO Data source: FileRecordDataSvc output: DATAFILE='PFN:"])
validateWithReference(preproc = preprocessor)
</text></argument>
  <argument name="prerequisites"><set>
    <tuple><text>brunel.brunel2010magoff</text><enumeral>PASS</enumeral></tuple>
  </set></argument>
</extension>
