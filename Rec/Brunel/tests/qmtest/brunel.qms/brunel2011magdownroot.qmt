<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension//en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
  <argument name="program"><text>gaudirun.py</text></argument>
  <argument name="args"><set>
    <text>$BRUNELOPTS/COLLISION11-Beam3500GeV-VeloClosed-MagDown.py</text>
    <text>$APPCONFIGOPTS/Brunel/MonitorExpress.py</text>
    <text>--option</text>
    <text>from Brunel.Configuration import *; Brunel().EvtMax=200; Brunel().Monitors=["SC","FPE"]; Brunel().OutputType="SDST"; Brunel().PackType="MDF"; LHCbApp().Persistency="ROOT"</text>
  </set></argument>
  <argument name="reference"><text>../refs/brunel2011magdown.ref</text></argument>
  <argument name="validator"><text>
from Brunel.QMTest.BrunelExclusions import preprocessor as BrunelPreprocessor
preprocessor = BrunelPreprocessor + \
  LineSkipper(["EventPersistenc...   INFO  'CnvServices':"]) + \
  LineSkipper(["EventPersistenc...   INFO Added successfully Conversion service:"])
validateWithReference(preproc = preprocessor)
</text></argument>
  <argument name="prerequisites"><set>
    <tuple><text>brunel.brunel2011magdown</text><enumeral>PASS</enumeral></tuple>
  </set></argument>
</extension>
