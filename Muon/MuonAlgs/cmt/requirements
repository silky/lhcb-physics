package MuonAlgs 
version v5r1

# ================ structure ===========================================
branches src cmt MuonAlgs options doc

# ================ own includes ========================================== 
include_dirs      $(MUONALGSROOT)     

# ================ used packages ========================================= 
use AIDA        v*	 -no_auto_imports
use MuonTools   v3r* 	Muon
use MuonEvent	v3r*	Event	

# build a component library
library MuonAlgs  *.cpp -import=AIDA

apply_pattern component_library library=MuonAlgs
