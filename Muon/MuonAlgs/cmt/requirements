package MuonAlgs 
version v4r0

# ================ structure ===========================================
branches src cmt MuonAlgs options doc

# ================ own includes ========================================== 
include_dirs      $(MUONALGSROOT)     

# ================ used packages ========================================= 
use AIDA        v*	 -no_auto_imports
use MuonTools   v3r* 	Muon
use MuonEvent	v2r*	Event	

# build a component library
library MuonAlgs  *.cpp -import=AIDA

apply_pattern package_Clinkopts
apply_pattern packageShr

#===============================================================
private
apply_pattern package_Cshlibflags    
