package MuonBackground 
version v1r3

# ================ structure ===========================================
branches src cmt options doc

# ================ own includes ========================================== 
include_dirs      $(MUONBACKGROUNDROOT)     

# ================ used packages ========================================= 
use AIDA        v*	 -no_auto_imports
use MuonTools	v3r*	Muon
use MuonEvent	v2r*	Event

# build a component library
library MuonBackground  *.cpp -import=AIDA

apply_pattern package_Clinkopts
apply_pattern packageShr

#===============================================================
private
