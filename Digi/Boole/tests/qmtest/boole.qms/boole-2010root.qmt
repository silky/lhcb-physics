<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension//en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
  <argument name="program"><text>gaudirun.py</text></argument>
  <argument name="args"><set>
    <text>$BOOLEROOT/tests/options/testBoole-defaults.py</text>
    <text>$BOOLEOPTS/Boole-2010.py</text>
    <text>$BOOLEOPTS/2010-Files.py</text>
    <text>--option</text>
    <text>from Boole.Configuration import *; Boole().Persistency="ROOT"</text>
  </set></argument>
  <argument name="reference"><text>../refs/testBoole-2010.ref</text></argument>
  <argument name="validator"><text>
from Boole.QMTest.BooleExclusions import preprocessor as BoolePreprocessor
preprocessor = BoolePreprocessor + \
  LineSkipper(["EventSelector        INFO Stream:EventSelector.DataStreamTool_1 Def:DATAFILE="]) + \
  LineSkipper(["PoolRootEvtCnvSvc    INFO No Records /FileRecords present in:00000000-0000-0000-0000-000000000000"]) + \
  LineSkipper(["EventPersistenc...   INFO  'CnvServices':"]) + \
  LineSkipper(["EventPersistenc...   INFO Added successfully Conversion service:"]) + \
  LineSkipper(["DigiWriter           INFO Data source: EventDataSvc output: DATAFILE='PFN:"])
validateWithReference(preproc = preprocessor)
</text></argument>
  <argument name="prerequisites"><set>
    <tuple><text>boole.boole-2010</text><enumeral>PASS</enumeral></tuple>
  </set></argument>
</extension>
