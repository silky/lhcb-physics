                        , "HLTFilterBd2D0Kstar"
                       , "HLTFilterBd2DstarPi"
                       , "HLTFilterBs2DsH"
                       , "HLTFilterBs2PhiPhi"
			,"HLTFilterBs2PhiGamma"
		       , "HLTFilterB2MuMu"
