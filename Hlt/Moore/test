// Standard Gaudi configuration and additional DLLs are in
// DaVinciCommon.
#pragma print off // Printing switched off in MainRTTC.opts Line 13 Print level is now 1
#pragma print on // Printing switched on in MainRTTC.opts Line 15 Print level is now 0
//--------------------------------------------------------------
// Run HLT
// --> Including file $HLTGENERICROOT/options/HltGeneric.opts


ApplicationMgr.DLLs   += { "HltGeneric", "TrgMuon" };

// Decoding is done twice (also in FullTracking)
HLTGenericSeq.Members =  {   
  "TrgMuonRec"
  , "HltGenericSelection"
  ,"DecodeTTFromRaw/GenMakeTTHit"
  ,"TrgVeloTT/GenVeloTT"
  ,"TrgMuonRecover"
  ,"FwdHitFromRawBuffer/GenMakeTHit"
  ,"TrgForwardAlg/GenForward"
  ,"TrgMuonRefine"
  ,"TrgInsertTrackErrParam/GenErrParam"
  ,"TrgParticleMaker/GenParticleMaker"
  ,"HltGenericDecision"
};


// TrgMuonRec preserve muon segments
TrgMuonRec.StoreTracks = true;

// generic selction, locate tracks
HltGenericSelection.OutputTracksName = "HltGeneric/Velo"; // "HltGeneric/Velo"

// the track containers by HltGeneric reconstruction
GenVeloTT.InputTracksName  = "HltGeneric/Velo"; // "HltGeneric/Velo"
GenVeloTT.OutputTracksName = "HltGeneric/VeloTT"; // "HltGeneric/VeloTT"
GenForward.InputTracksName = "HltGeneric/VeloTT"; // "HltGeneric/VeloTT"
GenForward.OuputTracksName = "HltGeneric/Long"; // "HltGeneric/Long"


// Parameters for the Hugo's error correction of tracks
GenErrParam.InputTracks = "HltGeneric/Long";
// GenErrParam.par1x=0.0138*mm;
// GenErrParam.par2x=0.00262*mm;
// GenErrParam.par0y=0.0164*mm;
// GenErrParam.par1y=0.0138*mm;
// GenErrParam.par2y=0.00262*mm;

// Generic TrgParticleMaker options
GenParticleMaker.InputTracks = "HltGeneric/Long";
GenParticleMaker.StoredTracks = "HltGeneric/Particles";
GenParticleMaker.OutputRoot = "/Event/Phys/HltGeneric/";
GenParticleMaker.MakeKaon = false;
GenParticleMaker.MakeNeutrals = false; 
GenParticleMaker.Exclusive = true;

HltGenericDecision.PhysDesktop.InputPrimaryVertices = "None";
HltGenericDecision.VertexFitter        = "TrgVertexFitter" ;

