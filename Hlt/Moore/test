// Shared Phi
//------------------------------------------------------------------------------
HLTMakeResonances.Members += {"MakeResonances/HLTSharedPhi"} ;
HLTSharedPhi.PhysDesktop.InputLocations = {"Phys/HLTPreselKaons"};
HLTSharedPhi.DecayDescriptor = "phi(1020) -> K+ K-" ;
HLTSharedPhi.Window = 30*MeV ; // 
HLTSharedPhi.MotherFilter.Selections = { "phi(1020) : VtxFilterCriterion" };
//                                                     , KinFilterCriterion 
//                                                     , PVIPFilterCriterion" };
HLTSharedPhi.MotherFilter.VtxFilterCriterion.MaxChi2 = 200; // hlt tuned: 200, hlt presel: 200 
//HLTSharedPhi.MotherFilter.KinFilterCriterion.MinPt = 0.; // MeV
//HLTSharedPhi.MotherFilter.KinFilterCriterion.MinMomentum = 0.; // MeV
//HLTSharedPhi.MotherFilter.PVIPFilterCriterion.MinIPsignif = 0.; // sigma
//------------------------------------------------------------------------------

