HLTFullTracking.Members += { "TrgInsertTrackErrParam" };

// TrgInsertTrackErrParam.Par0x=0.0130*mm;
// TrgInsertTrackErrParam.Par1x=0.0165*mm;
// TrgInsertTrackErrParam.Par2x=0.00179*mm;
// TrgInsertTrackErrParam.Par0y=0.0130*mm;
// TrgInsertTrackErrParam.Par1y=0.0165*mm;
// TrgInsertTrackErrParam.Par2y=0.00179*mm;

// <-- End of including file $HLTSELECTIONSROOT/options/TrgInsertTrackErrParam.opts

//
// Define PV
HLTSelections.Members += { "ResetPVLocation/SetHLTPV" };
SetHLTPV.PVLocation = "/Event/Phys/Trg/Vertices" ;

// Check it is there
HLTSelections.Members += { "CheckPV" };

//ApplicationMgr.DLLs += { "PhysSelections" };

HLTSelections.Members += { "GaudiSequencer/CopyParticles" };
CopyParticles.IgnoreFilterPassed = true ; // don't stop yet
//------------------------------------------------------------------------------
// copy all pions
CopyParticles.Members += { "PIDFilter/HLTPions" };
HLTPions.PhysDesktop.InputLocations = { "Phys/Trg" };
HLTPions.ParticleNames = { "pi+", "pi-"};
//------------------------------------------------------------------------------
// copy all kaons
CopyParticles.Members += {"PIDFilter/HLTKaons"};
HLTKaons.PhysDesktop.InputLocations = { "Phys/Trg" };
HLTKaons.ParticleNames = { "K+", "K-"};
//------------------------------------------------------------------------------
// copy all photons
CopyParticles.Members += { "PIDFilter/HLTPhotons" };
HLTPhotons.PhysDesktop.InputLocations = { "Phys/Trg" } ; 
HLTPhotons.ParticleNames = { "gamma"};


// <-- End of including file $HLTSELECTIONSROOT/options/TrgTracks.opts

