//==============================================================================
// All D0 
//   There is no D~0. All D0 are D0!
//==============================================================================
//------------------------------------------------------------------------------
//  D0 -> h+h+
//------------------------------------------------------------------------------
HLTMakeResonances.Members += {"MakeResonances/HLTSharedD02KH"} ;
HLTSharedD02KH.PhysDesktop.InputLocations = {"Phys/HLTPreselPions", "Phys/HLTPreselKaons"};
HLTSharedD02KH.DecayDescriptor = "D0 -> h+ h-" ;
HLTSharedD02KH.DecayDescriptors = { "D0 -> K+ pi-", "D0 -> pi+ K-", "D0 -> K+ K-" } ;
HLTSharedD02KH.DaughterFilter.Selections = { "K+ : KinFilterCriterion", 
                                             "pi+ : KinFilterCriterion"};
//HLTSharedD02KH.DaughterFilter.KinFilterCriterion.MinPt = 300.*MeV; // hlt tuned: 300 MeV, hlt presel: 0 MeV, no offline cut
HLTSharedD02KH.DaughterFilter.KinFilterCriterion.MinMomentum = 1000.*MeV ; // hlt tuned: 1 GeV, hlt presel: 1 GeV, no offline cut
//HLTSharedD02KH.DaughterFilter.PVIPFilterCriterion.MinIPsignif = 1.; // hlt tuned: 1, hlt presel: 0, no offline cut
HLTSharedD02KH.Window = 50*MeV ; //
HLTSharedD02KH.MinPt = 1500.*MeV; // MeV
HLTSharedD02KH.MotherFilter.Selections = {  "D0 : VtxFilterCriterion"}; 
//						,    PVIPFilterCriterion
HLTSharedD02KH.MotherFilter.VtxFilterCriterion.MaxChi2 = 50; // hlt tuned: 200, hlt presel: 200 
//HLTSharedD02KH.MotherFilter.KinFilterCriterion.MinMomentum = 0.; // MeV
//HLTSharedD02KH.MotherFilter.PVIPFilterCriterion.MinIPsignif = 0.; // sigma

//------------------------------------------------------------------------------
//  D0 -> pi+pi-
//------------------------------------------------------------------------------
HLTMakeResonances.Members += {"MakeResonances/HLTLooseD02PiPi"} ;
HLTLooseD02PiPi.PhysDesktop.InputLocations = {"Phys/HLTPreselPions"};
HLTLooseD02PiPi.DecayDescriptor = "D0 -> pi+ pi-" ;
HLTLooseD02PiPi.DaughterFilter.Selections = { "pi+ : KinFilterCriterion, PVIPFilterCriterion"};
//HLTLooseD02PiPi.DaughterFilter.KinFilterCriterion.MinPt = 300.*MeV; // hlt tuned: 300 MeV, hlt presel: 0 MeV, no offline cut
HLTLooseD02PiPi.DaughterFilter.KinFilterCriterion.MinMomentum = 1000.*MeV ; // hlt tuned: 1 GeV, hlt presel: 1 GeV, no offline cut
HLTLooseD02PiPi.DaughterFilter.PVIPFilterCriterion.MinIPsignif = 2.; // hlt tuned: 1, hlt presel: 0, no offline cut
HLTLooseD02PiPi.UpperWindow = 50*MeV ; // to be revised
HLTLooseD02PiPi.LowerWindow = 1000*MeV ; // to be revised
HLTLooseD02PiPi.MinPt = 1500.*MeV; 
HLTLooseD02PiPi.MotherFilter.Selections = {  "D0 : VtxFilterCriterion"}; 
//						,    PVIPFilterCriterion
HLTLooseD02PiPi.MotherFilter.VtxFilterCriterion.MaxChi2 = 50; // hlt tuned: 200, hlt presel: 200 
//HLTLooseD02PiPi.MotherFilter.KinFilterCriterion.MinMomentum = 0.; // MeV
//HLTLooseD02PiPi.MotherFilter.PVIPFilterCriterion.MinIPsignif = 0.; // sigma


// <-- End of including file $HLTSELECTIONSROOT/options/HLTSharedD0.opts

//==============================================================================
// HLT Selection for generic D* stream
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselDstar.opts


//==============================================================================
// HLT Selection of D*(2010)- -> (D~0 -> h+ h-)  pi-) pi+ 
//==============================================================================
// Author: P. Koppenburg   12/11/2004

HLTSelections.Members += {"GaudiSequencer/HLTselDstar"};
HLTselDstar.IgnoreFilterPassed = false ;
HLTselDstar.MeasureTime = true ;

//------------------------------------------------------------------------------
// HLTFilterDstar  !! Take care of wrong-charge too
// Might be used for Bd->Dstar pi too?
HLTselDstar.Members += {"MakeResonances/HLTFilterDstar"};
HLTFilterDstar.PhysDesktop.InputLocations = {"Phys/HLTLooseD02PiPi", "Phys/HLTDetachedPions"}; // no preselection!
HLTFilterDstar.DecayDescriptor = " [ D*(2010)- -> D~0 pi- ]cc ";
HLTFilterDstar.DecayDescriptors = {" D*(2010)- -> D0 pi- ", " D*(2010)+ -> D0 pi+ " }; // remember that there are no D~0!
// daughters cuts
HLTFilterDstar.DaughterFilter.Selections = { "D0 : KinFilterCriterion, FlightDistanceFilterCriterion"}; // no cut on pi
HLTFilterDstar.DaughterFilter.KinFilterCriterion.MinPt = 2000*MeV ; // From Thomas
//HLTFilterDstar.DaughterFilter.FlightDistanceFilterCriterion.MinSignedFPV =  5*mm ; // From Thomas
HLTFilterDstar.DaughterFilter.FlightDistanceFilterCriterion.MinFSPV =  5 ; // sigma
// window
HLTFilterDstar.UpperWindow = 50*MeV ; // to be revised
HLTFilterDstar.LowerWindow = 300*MeV ; // to be revised
HLTFilterDstar.MinPt = 2000*MeV ;  // From Thomas
HLTFilterDstar.KillOverlap = true ; 
// mother cuts
HLTFilterDstar.MotherFilter.Selections = {"D*(2010)- : VtxFilterCriterion, 
                                                       MassDifferenceFilterCriterion"};
HLTFilterDstar.MotherFilter.VtxFilterCriterion.MaxChi2 = 60.; // hlt tuned: 60, hlt presel: 200, offline is 15
HLTFilterDstar.MotherFilter.MassDifferenceFilterCriterion.Window = 10.*MeV; // Thomas asked for 5
HLTFilterDstar.MotherFilter.MassDifferenceFilterCriterion.DaughterName =  "D0"  ;
HLTFilterDstar.MotherFilter.MassDifferenceFilterCriterion.MotherName =  "D*(2010)-"  ;                                                                                                                                                                        
//HLTFilterDstar.MotherFilter.KinFilterCriterion.MinPt = 2000 ;  // From Thomas

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselDstar.opts

//==============================================================================
// Exclusive Selections
//==============================================================================
HLTSelections.Members += {"GaudiSequencer/HLTExclusive"};
HLTExclusive.IgnoreFilterPassed = true ;
HLTExclusive.MeasureTime = true ;
//==============================================================================
// HLT Selection of B -> H H
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselB2HH.opts


//==============================================================================
// HLT Selection of B -> H H
//==============================================================================
// Author: Luis Fernandez, 13/08/2004
// Author: P. Koppenburg,  12/10/2004
// Author: P. Koppenburg,  05/01/2005 - use FilterDesktop

//------------------------------------------------------------------------------
// Filter All B -> HH
//------------------------------------------------------------------------------
HLTExclusive.Members += {"GaudiSequencer/HLTselB2HH"};
HLTselB2HH.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselB2HH.MeasureTime = true ;

//------------------------------------------------------------------------------
HLTselB2HH.Members += {"MakeResonances/HLTFilterB2HH"} ;
HLTFilterB2HH.PhysDesktop.InputLocations = { "Phys/HLTPreselPions" };
HLTFilterB2HH.DecayDescriptor = "B0 -> pi+ pi-" ;
HLTFilterB2HH.DaughterFilter.Selections = {"pi+ : KinFilterCriterion/PiKin,
				          PVIPFilterCriterion/PiPVIP"};
HLTFilterB2HH.DaughterFilter.PiKin.MinPt = 1000.*MeV; // hlt tuned: 800 MeV, hlt presel: 0 MeV, offline is 800-1400 MeV, depending on channel
HLTFilterB2HH.DaughterFilter.PiKin.MinMomentum = 2500.*MeV; // hlt tuned: 2500 MeV, hlt presel: 0 MeV, offline is 2500-2750 MeV
HLTFilterB2HH.DaughterFilter.PiPVIP.MinIPsignif = 4.; // hlt tuned: 4, hlt presel: 0, offline is 5-7

HLTFilterB2HH.Window = 600*MeV;// to take into account the K and Bs mass. Largely enough.
HLTFilterB2HH.MinPt = 1000.; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, offline is 1000-1600 MeV

HLTFilterB2HH.MotherFilter.Selections = {"B0 : VtxFilterCriterion, 
                                         FlightDistanceFilterCriterion,
                                         PVIPFilterCriterion,
                                         Momentum2FlightAngleFilterCriterion"};
HLTFilterB2HH.MotherFilter.VtxFilterCriterion.MaxChi2 = 20.; // hlt tuned: 10, hlt presel: 200, offline is 4-5
HLTFilterB2HH.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterB2HH.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 10. ; // hlt tuned: 8, hlt presel: 0, offline is 14-20
HLTFilterB2HH.MotherFilter.PVIPFilterCriterion.MaxIPsignif = 4.; // hlt tuned: 5 -> 4, hlt presel: 200, offline is 2.25-2.75
HLTFilterB2HH.MotherFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterB2HH.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: 0.999, hlt presel: -2.0, no offline cut



// <-- End of including file $HLTSELECTIONSROOT/options/HLTselB2HH.opts

//==============================================================================
// HLT Selection of B_s0 -> phi(1020) phi(1020)
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBs2PhiPhi.opts


//==============================================================================
// HLT Selection of B_s0 -> phi(1020) phi(1020)
//==============================================================================
// Author: Luis Fernandez, 13/08/2004
// Author: P. Koppenburg,  04/01/2005, make it a single MakeResonances algorithm

HLTExclusive.Members += {"GaudiSequencer/HLTselBs2PhiPhi"};
HLTselBs2PhiPhi.IgnoreFilterPassed = true ;   // Act as Sequencer
HLTselBs2PhiPhi.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection on the phi daughters (the two kaons)
HLTselBs2PhiPhi.Members += {"MakeResonances/HLTFilterBs2PhiPhi"} ;
HLTFilterBs2PhiPhi.PhysDesktop.InputLocations = {"Phys/HLTSharedPhi"}; // filter the Phis
//
// Filter daughters
HLTFilterBs2PhiPhi.DaughterFilter.Selections = { "phi(1020) : MassFilterCriterion/PhiMass, 
                                                             VtxFilterCriterion/PhiVtx,
                                                             KinFilterCriterion/PhiKin, 
                                                             PVIPFilterCriterion/PhiPVIP",
                                                "K+ : KinFilterCriterion/KKin, PVIPFilterCriterion/KPVIP" };
HLTFilterBs2PhiPhi.DaughterFilter.FilterByDescendents = true ;
// filter K
HLTFilterBs2PhiPhi.DaughterFilter.KKin.MinPt = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2PhiPhi.DaughterFilter.KKin.MinMomentum = 1000.*MeV; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, no offline cut
//HLTFilterBs2PhiPhi.DaughterFilter.KPVIP.MinIPsignif = 1.5; // hlt tuned: 1, hlt presel: 0, offline is 2
// filter phi
HLTFilterBs2PhiPhi.DaughterFilter.PhiMass.Window = 24*MeV; // hlt tuned: 24 MeV, hlt presel: 50 MeV, offline is 12 MeV
HLTFilterBs2PhiPhi.DaughterFilter.PhiVtx.MaxChi2 = 100; // hlt tuned: 100, hlt presel: 200, no offline cut
HLTFilterBs2PhiPhi.DaughterFilter.PhiKin.MinPt = 0.; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2PhiPhi.DaughterFilter.PhiKin.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
//HLTFilterBs2PhiPhi.DaughterFilter.PhiPVIP.MinIPsignif = 2.0 ; // hlt tuned: 2, hlt presel: 0, no offline cut
//
// combine
HLTFilterBs2PhiPhi.DecayDescriptor = "B_s0 -> phi(1020) phi(1020)";
HLTFilterBs2PhiPhi.Window = 500*MeV ;
HLTFilterBs2PhiPhi.KillOverlap = true ;
//
// Bs cuts
HLTFilterBs2PhiPhi.MotherFilter.Selections = {"B_s0 : VtxFilterCriterion,
						      FlightDistanceFilterCriterion,
						      Momentum2FlightAngleFilterCriterion,
						      PVIPFilterCriterion"};
HLTFilterBs2PhiPhi.MotherFilter.VtxFilterCriterion.MaxChi2 = 80; // hlt tuned: 200, hlt presel: 200, offline is 100
//HLTFilterBs2PhiPhi.MotherFilter.KinFilterCriterion.MinPt = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
//HLTFilterBs2PhiPhi.MotherFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2PhiPhi.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBs2PhiPhi.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 6.; // hlt tuned: 5, hlt presel: 0, no offline cut
HLTFilterBs2PhiPhi.MotherFilter.PVIPFilterCriterion.MaxIPsignif = 4.0 ; // hlt tuned: 10, hlt presel: 0, no offline cut
HLTFilterBs2PhiPhi.MotherFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBs2PhiPhi.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999 ; // hlt tuned: 0.999, hlt presel: 0.0, offline is 10mrad (0.9999)
//------------------------------------------------------------------------------


// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBs2PhiPhi.opts

//==============================================================================
// HLT Selection of B0 -> (D~0 -> K+ pi-) (K*(892)0 -> K+ pi-)
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBd2D0Kstar.opts


//==============================================================================
// HLT Selection of B0 -> (D~0 -> K+ pi-) (K*(892)0 -> K+ pi-) and cc
//==============================================================================
// Author: Luis Fernandez, 13/08/2004

HLTExclusive.Members += {"GaudiSequencer/HLTselBd2D0Kstar"};
HLTselBd2D0Kstar.IgnoreFilterPassed = false ;  // act as real sequencer
HLTselBd2D0Kstar.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection on the D0 daughters 
// Do this first as it is less efficient
HLTselBd2D0Kstar.Members += {"FilterDesktop/FilterD0ForHLTBd2D0Kstar"} ;
FilterD0ForHLTBd2D0Kstar.PhysDesktop.InputLocations = {"Phys/HLTSharedD02KH"}; // , "Phys/HLTLooseD02PiPi"};

FilterD0ForHLTBd2D0Kstar.Filter.Selections = { "D0 : MassFilterCriterion, 
                                                     VtxFilterCriterion, 
						     KinFilterCriterion/D0Kin, 
						     FlightDistanceFilterCriterion", 
                                           "K+ : KinFilterCriterion, PVIPFilterCriterion", 
					   "pi+ : KinFilterCriterion, PVIPFilterCriterion" };
FilterD0ForHLTBd2D0Kstar.Filter.FilterByDescendents = true ; // use pi and K
// pi and K cuts
//FilterD0ForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinPt = 300.*MeV; // hlt tuned: 300 MeV, hlt presel: 0 MeV, no offline cut
FilterD0ForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinMomentum = 1500.*MeV; // hlt tuned: 1500 MeV, hlt presel: 1 GeV, no offline cut
FilterD0ForHLTBd2D0Kstar.Filter.PVIPFilterCriterion.MinIPsignif = 1.0; // hlt tuned: 1.5, hlt presel: 1.0, offline is larger than 2.4
// D0 cuts
FilterD0ForHLTBd2D0Kstar.Filter.MassFilterCriterion.Window = 50*MeV; // hlt tuned: 45 MeV, hlt presel: 200 MeV, offline is 50 MeV (in note) or 30 MeV in TDRsel
FilterD0ForHLTBd2D0Kstar.Filter.VtxFilterCriterion.MaxChi2 = 15.; // hlt tuned: 30, hlt presel: 50, offline is 20
FilterD0ForHLTBd2D0Kstar.Filter.D0Kin.MinPt = 2000.*MeV; // hlt tuned: 500 MeV, hlt presel: 0 MeV, offline is 900 MeV (in note) or 500 in TDRsel
FilterD0ForHLTBd2D0Kstar.Filter.D0Kin.MinMomentum = 0.*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
FilterD0ForHLTBd2D0Kstar.Filter.FlightDistanceFilterCriterion.MinFSPV = 3.0; 
FilterD0ForHLTBd2D0Kstar.Filter.FlightDistanceFilterCriterion.CutBestPV = true;
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// Apply some pre-selection on the Kstar daughters 
HLTselBd2D0Kstar.Members += {"FilterDesktop/FilterKstarForHLTBd2D0Kstar"} ;
FilterKstarForHLTBd2D0Kstar.PhysDesktop.InputLocations = {"Phys/HLTSharedKstar"};

FilterKstarForHLTBd2D0Kstar.Filter.Selections = { "K*(892)0 : MassFilterCriterion, 
                                                         VtxFilterCriterion,
                                                         KinFilterCriterion/KstarKin, 
                                                         PVIPFilterCriterion/KstarPVIP,
							 FlightDistanceFilterCriterion", 
//                                               "K+ : KinFilterCriterion, PVIPFilterCriterion", 
//					       "pi+ : KinFilterCriterion, PVIPFilterCriterion"
                                               "K+ : KinFilterCriterion", 
					       "pi+ : KinFilterCriterion"
					       };
// pi and K
FilterKstarForHLTBd2D0Kstar.Filter.FilterByDescendents = true ; // use pi and K 
//FilterKstarForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinPt = 300.; // hlt tuned: 300 MeV, hlt presel: 0 MeV, no offline cut
FilterKstarForHLTBd2D0Kstar.Filter.KinFilterCriterion.MinMomentum = 1500.; // hlt tuned: 1500 MeV, hlt presel: 1 GeV, no offline cut
//FilterKstarForHLTBd2D0Kstar.Filter.PVIPFilterCriterion.MinIPsignif = 1.5; // hlt tuned: 1.5, hlt presel: 1.0, offline is 2.4
// K*
FilterKstarForHLTBd2D0Kstar.Filter.MassFilterCriterion.Window = 120*MeV; // hlt tuned: 120 MeV, hlt presel: 200 MeV, offline is 80 MeV (in note) 
FilterKstarForHLTBd2D0Kstar.Filter.VtxFilterCriterion.MaxChi2 = 20.; // hlt tuned: 30, hlt presel: 50, offline is 20
FilterKstarForHLTBd2D0Kstar.Filter.KstarKin.MinPt = 1300.0*MeV; // hlt tuned: 500 MeV, hlt presel: 0 MeV, offline is 900 MeV (in note) or 500 in TDRsel
FilterKstarForHLTBd2D0Kstar.Filter.KstarKin.MinMomentum = 0.*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
FilterKstarForHLTBd2D0Kstar.Filter.KstarPVIP.MinIPsignif = 0.0 ; // hlt tuned: 0 MeV, hlt presel: 0, no offline cut
FilterKstarForHLTBd2D0Kstar.Filter.FlightDistanceFilterCriterion.CutBestPV = true;
FilterKstarForHLTBd2D0Kstar.Filter.FlightDistanceFilterCriterion.MinFSPV = 1.0; // hlt tuned: 0, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
// Bd2D0Kstar 
HLTselBd2D0Kstar.Members += {"MakeResonances/HLTFilterBd2D0Kstar"};
HLTFilterBd2D0Kstar.PhysDesktop.InputLocations = {"Phys/FilterKstarForHLTBd2D0Kstar", "Phys/FilterD0ForHLTBd2D0Kstar"};

HLTFilterBd2D0Kstar.DecayDescriptor = "[B0 -> D~0 K*(892)0]cc";
HLTFilterBd2D0Kstar.DecayDescriptors = { "B0 -> D0 K*(892)0", "B~0 -> D0 K*(892)~0" }; // No D~0!
HLTFilterBd2D0Kstar.Window = 500*MeV; // hlt tuned: 500 MeV, hlt presel: 500 MeV, offline tight is 50 MeV, loose is 500 MeV
//HLTFilterBd2D0Kstar.MinPt = 2000.*MeV; // hlt tuned: 5 GeV, hlt presel: 0 MeV, no offline cut
HLTFilterBd2D0Kstar.KillOverlap = true ;
// no daughter selections
HLTFilterBd2D0Kstar.MotherFilter.Selections = {"B0 : VtxFilterCriterion, 
                                                  FlightDistanceFilterCriterion,
                                                  PVIPFilterCriterion,
                                                  Momentum2FlightAngleFilterCriterion"};

HLTFilterBd2D0Kstar.MotherFilter.VtxFilterCriterion.MaxChi2 = 50.; // hlt tuned: 200, hlt presel: 200, offline is 10 (in note) or 15 in TDRsel
//HLTFilterBd2D0Kstar.MotherFilter.KinFilterCriterion.MinMomentum = 0.*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBd2D0Kstar.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBd2D0Kstar.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 6.; // hlt tuned: 6, hlt presel: 0, no offline cut
HLTFilterBd2D0Kstar.MotherFilter.PVIPFilterCriterion.MaxIPsignif = 4. ; // hlt tuned: 10, hlt presel: 20, offline is 3.8 (in note) or 4.0 in TDRsel
HLTFilterBd2D0Kstar.MotherFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBd2D0Kstar.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: 0.999, hlt presel: 0.0, offline is 45mrad or 0.9999
//------------------------------------------------------------------------------

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBd2D0Kstar.opts

//==============================================================================
// HLT Selection of B0 -> (D*(2010)- -> (D~0 -> K+ pi-)  pi-) pi+ 
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBd2DstarPi.opts


//==============================================================================
// HLT Selection of B0 -> (D*(2010)- -> (D~0 -> K+ pi-)  pi-) pi+ 
//==============================================================================
// Author: Luis Fernandez, 13/08/2004

HLTExclusive.Members += {"GaudiSequencer/HLTselBd2DstarPi"};
HLTselBd2DstarPi.IgnoreFilterPassed = false ;  // act as real sequencer
HLTselBd2DstarPi.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection on the D0 daughters 
HLTselBd2DstarPi.Members += { "FilterDesktop/FilterD0ForHLTBd2DstarPi" } ;
FilterD0ForHLTBd2DstarPi.PhysDesktop.InputLocations = { "Phys/HLTSharedD02KH" };//, "Phys/HLTLooseD02PiPi"};
FilterD0ForHLTBd2DstarPi.Filter.Selections = { "D0 : MassFilterCriterion,
                                                  VtxFilterCriterion,
						  KinFilterCriterion/D0Kin", 
                                           "K+ : KinFilterCriterion", 
					   "pi- : KinFilterCriterion" };
// use pi, K to filter D
FilterD0ForHLTBd2DstarPi.Filter.FilterByDescendents = true ; // use pi and K
//FilterD0ForHLTBd2DstarPi.Filter.KinFilterCriterion.MinPt = 300. ; // hlt tuned: 300 MeV, hlt presel: 0 MeV, offline cuts are 150 MeV for pi and 200 MeV for K
FilterD0ForHLTBd2DstarPi.Filter.KinFilterCriterion.MinMomentum = 1000. ; // hlt tuned: 1 GeV, hlt presel: 1 GeV, no offline cut
//FilterD0ForHLTBd2DstarPi.Filter.PVIPFilterCriterion.MinIPsignif = 2.; // hlt tuned: 1, hlt presel: 0, no offline cut
// filter D
FilterD0ForHLTBd2DstarPi.Filter.MassFilterCriterion.Window = 30*MeV; // hlt tuned: 30 MeV, hlt presel: 200 MeV, offline is 11 MeV
FilterD0ForHLTBd2DstarPi.Filter.VtxFilterCriterion.MaxChi2 = 40.; // hlt tuned: 80, hlt presel: 200, offline is 40
FilterD0ForHLTBd2DstarPi.Filter.D0Kin.MinMomentum = 1500. ; // in Shared D0 anyway
//FilterD0ForHLTBd2DstarPi.Filter.D0Kin.MinPt = 0.*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
//FilterD0ForHLTBd2DstarPi.Filter.D0Kin.MinMomentum = 0.*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
//FilterD0ForHLTBd2DstarPi.Filter.D0PVIP.MinIPsignif = 0.0 ; // hlt tuned: 0, hlt presel: 0,no offline cut
//FilterD0ForHLTBd2DstarPi.Filter.D0Flight.CutBestPV = true;
//FilterD0ForHLTBd2DstarPi.Filter.D0Flight.MinFSPV = 0.; // hlt tuned: 0, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
// Make D*
//------------------------------------------------------------------------------
HLTselBd2DstarPi.Members += {"MakeResonances/HLTDstarForBd2DstarPi"};
HLTDstarForBd2DstarPi.PhysDesktop.InputLocations = {"Phys/FilterD0ForHLTBd2DstarPi", "Phys/HLTDetachedPions"}; // also VTT
HLTDstarForBd2DstarPi.DecayDescriptor = " [ D*(2010)- -> D~0 pi- ]cc ";
HLTDstarForBd2DstarPi.DecayDescriptors = {" D*(2010)- -> D0 pi- ", " D*(2010)+ -> D0 pi+ " }; // No D~0 !
// Apply some pre-selection cuts to the soft pion. No additional cuts on D0
// Maybe use upstream tracks?
HLTDstarForBd2DstarPi.DaughterFilter.Selections = {"pi+ : KinFilterCriterion, PVIPFilterCriterion"};
HLTDstarForBd2DstarPi.DaughterFilter.KinFilterCriterion.MinPt = 0.*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTDstarForBd2DstarPi.DaughterFilter.KinFilterCriterion.MinMomentum = 0.*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTDstarForBd2DstarPi.DaughterFilter.PVIPFilterCriterion.MaxIP = 60.*mm; // hlt tuned: 60mm, hlt presel: 60mm, offline cut is 9mm
// mass
HLTDstarForBd2DstarPi.Window = 50*MeV; // hlt tuned: 40 MeV, hlt presel: 200 MeV, offline is 150 MeV for D* 
HLTDstarForBd2DstarPi.MinPt = 800.*MeV; // hlt tuned: 500 MeV, hlt presel: 0 MeV, offline is 1400 MeV
HLTDstarForBd2DstarPi.KillOverlap = true ;
// D* cuts
HLTDstarForBd2DstarPi.MotherFilter.Selections = {"D*(2010)- : VtxFilterCriterion" };
HLTDstarForBd2DstarPi.MotherFilter.VtxFilterCriterion.MaxChi2 = 100; // hlt tuned: 60, hlt presel: 200, offline is 15
//HLTDstarForBd2DstarPi.MotherFilter.PVIPFilterCriterion.MinIPsignif = 0.0 ; // hlt tuned: 0, hlt presel: 0, no offline cut
//HLTDstarForBd2DstarPi.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
//HLTDstarForBd2DstarPi.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 0.; // hlt tuned: 0, hlt presel: 0, no offline cut
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
// B -> D* pi
//------------------------------------------------------------------------------
HLTselBd2DstarPi.Members += {"MakeResonances/HLTFilterBd2DstarPi"};
HLTFilterBd2DstarPi.PhysDesktop.InputLocations = {"Phys/HLTDstarForBd2DstarPi", "Phys/HLTPreselPions"};
HLTFilterBd2DstarPi.DecayDescriptor = "[B0 -> D*(2010)- pi+]cc";
// cut on hard pi, not on D*
HLTFilterBd2DstarPi.DaughterFilter.Selections = { "pi+ : KinFilterCriterion, PVIPFilterCriterion" }; 
HLTFilterBd2DstarPi.DaughterFilter.KinFilterCriterion.MinPt = 1300.*MeV; // hlt tuned: 1300 MeV, hlt presel: 300 MeV, offline is 1300 MeV
HLTFilterBd2DstarPi.DaughterFilter.KinFilterCriterion.MinMomentum = 1000.*MeV; // hlt tuned: 1000 MeV, hlt presel: 1000 MeV, no offline cut
HLTFilterBd2DstarPi.DaughterFilter.PVIPFilterCriterion.MinIP = -1000.*mm; // hlt tuned: -1000, hlt presel: -1000, offline is 0.05 mm
HLTFilterBd2DstarPi.DaughterFilter.PVIPFilterCriterion.MinIPsignif = 2.0; // hlt tuned: 2.5, hlt presel: 0.5, offline is 0.05 mm
// mass
HLTFilterBd2DstarPi.Window = 500*MeV; // hlt tuned: 500 MeV, hlt presel: 500 MeV, offline tight is 50 MeV, loose is 200 MeV
HLTFilterBd2DstarPi.KillOverlap = true ; 
// cut on B
HLTFilterBd2DstarPi.MotherFilter.Selections = {"B0 : VtxFilterCriterion,
                                                     PVIPFilterCriterion,
                                                     FlightDistanceFilterCriterion,
                                                     Momentum2FlightAngleFilterCriterion"};

HLTFilterBd2DstarPi.MotherFilter.VtxFilterCriterion.MaxChi2 = 30.; // hlt tuned: 50, hlt presel: 200, offline is 12
//HLTFilterBd2DstarPi.MotherFilter.KinFilterCriterion.MinPt = 0.*MeV ; // hlt tuned: 1000 MeV, hlt presel: 0 MeV, no offline cut
//HLTFilterBd2DstarPi.MotherFilter.KinFilterCriterion.MinMomentum = 0*MeV; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBd2DstarPi.MotherFilter.PVIPFilterCriterion.MaxIPsignif = 5.0; // hlt tuned: 6 -> 4, hlt presel: 1000, offline is 0.05 mm
HLTFilterBd2DstarPi.MotherFilter.PVIPFilterCriterion.MaxIP = 30.*mm; // hlt presel: 30, offline is 0.05 mm
HLTFilterBd2DstarPi.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBd2DstarPi.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 5.; // hlt tuned: 5, hlt presel: 0 , no offline cut
HLTFilterBd2DstarPi.MotherFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBd2DstarPi.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: 0.999, hlt presel: 0.0, no offline cut
//------------------------------------------------------------------------------

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBd2DstarPi.opts

// For MC truth
// HLTSoftPionForHLTDstar.PhysDesktop.InputLocations = {"Phys/SelectBd2DstarPi"};
//==============================================================================
// HLT Selection of B_s0 -> (D_s- -> K+ K- pi-) h+
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTMakeDs.opts


//==============================================================================
// Form the D_s- -> K+ K- pi- and cc for Bs2DsH
//==============================================================================

HLTExclusive.Members += {"GaudiSequencer/HLTMakeDs"};
HLTMakeDs.IgnoreFilterPassed = true ;
HLTMakeDs.MeasureTime = true ;

//------------------------------------------------------------------------------
// THREE-BODY DECAY Ds, simple but CPU-consuming
//------------------------------------------------------------------------------
HLTMakeDs.Members += {"MakeResonances/HLTFilterDs2KKPi"};
HLTFilterDs2KKPi.PhysDesktop.InputLocations = {"Phys/HLTPreselKaons", "Phys/HLTPreselPions"};
HLTFilterDs2KKPi.DecayDescriptor = "[D_s- -> K+ K- pi-]cc";
// Cuts for pi and K
HLTFilterDs2KKPi.DaughterFilter.Selections = { "K- : KinFilterCriterion",   
                                               "pi+ : KinFilterCriterion"};
//HLTFilterDs2KKPi.DaughterFilter.KinFilterCriterion.MinPt = 300.0 * MeV; // hlt tuned: 300 MeV, hlt presel: 300 MeV, offline is 300 MeV
HLTFilterDs2KKPi.DaughterFilter.KinFilterCriterion.MinMomentum = 1500.0 * MeV; // hlt tuned: 1.5 MeV, hlt presel: 1 GeV, offline is 2 GeV
//HLTFilterDs2KKPi.DaughterFilter.PVIPFilterCriterion.MinIPsignif = 2.; // hlt tuned: 1, hlt presel: 0, no offline cut
// Cuts for combination
HLTFilterDs2KKPi.Window = 45*MeV ;// hlt tuned: 45 MeV, hlt presel: 100 MeV, offline is 15 MeV
HLTFilterDs2KKPi.MinPt = 1500.; // hlt tuned: 2500 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterDs2KKPi.KillOverlap = true ;
// Cuts for mother
HLTFilterDs2KKPi.MotherFilter.Selections = { "D_s- : VtxFilterCriterion, 
                                               FlightDistanceFilterCriterion,
                                               PVIPFilterCriterion,
                                               Momentum2FlightAngleFilterCriterion" };
HLTFilterDs2KKPi.MotherFilter.VtxFilterCriterion.MaxChi2 = 80; // hlt tuned: 40, hlt presel: 100, offline is 20
HLTFilterDs2KKPi.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterDs2KKPi.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 3.0; // hlt tuned: 3, hlt presel: 0, offline cut is 4.5
HLTFilterDs2KKPi.MotherFilter.PVIPFilterCriterion.MinIPsignif = 1.0 ; // hlt tuned: 0, hlt presel: 0, offline cut is 2.0 but w.r.t best PV ...
HLTFilterDs2KKPi.MotherFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterDs2KKPi.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle = -2.0; // hlt tuned: -2.0, hlt presel: -2.0, no offline cut
//------------------------------------------------------------------------------


// <-- End of including file $HLTSELECTIONSROOT/options/HLTMakeDs.opts

// --> Including file $HLTSELECTIONSROOT/options/HLTselBs2DsH.opts


//==============================================================================
// HLT Selection of B_s0 -> (D_s- -> K+ K- pi-) h+
//==============================================================================
// Author: Luis Fernandez, 13/08/2004
// P. Koppenburg, 04/01/2005 adapted to a single MakeResonances algorithm
//==============================================================================

HLTExclusive.Members += {"GaudiSequencer/HLTselBs2DsH"};
HLTselBs2DsH.IgnoreFilterPassed = true ; // 
HLTselBs2DsH.MeasureTime = true ;

//==============================================================================
// B_s0 -> (D_s- -> K+ K- pi-) [pi+,K+] 
//==============================================================================
HLTselBs2DsH.Members += {"MakeResonances/HLTFilterBs2DsH"} ;
//HLTFilterBs2DsH.PhysDesktop.InputLocations = {"Phys/HLTFilterDs2KKPi","Phys/HLTPreselPions","Phys/HLTPreselKaons"}; // only look at pions
HLTFilterBs2DsH.PhysDesktop.InputLocations = {"Phys/HLTFilterDs2KKPi","Phys/HLTPreselPions"}; // only look at pions
HLTFilterBs2DsH.DecayDescriptor = "[B_s0 -> D_s- pi+]cc" ;
//HLTFilterBs2DsH.DecayDescriptors = {"[B_s0 -> D_s- pi+]cc", "[B_s0 -> D_s- K+]cc" } ;
//
// select bachelor pi
HLTFilterBs2DsH.DaughterFilter.Selections = { "pi+ : KinFilterCriterion, PVIPFilterCriterion"
//                                              ,"K+ : KinFilterCriterion, PVIPFilterCriterion"
					      }; // no more Ds cut
HLTFilterBs2DsH.DaughterFilter.KinFilterCriterion.MinPt = 1300.*MeV; // hlt tuned: 1300 MeV, hlt presel: 300 MeV, offline is 700 MeV
HLTFilterBs2DsH.DaughterFilter.KinFilterCriterion.MinMomentum = 2500.*MeV; // hlt tuned: 2.5 GeV, hlt presel: 1 GeV, offline is 2 GeV
//HLTFilterBs2DsH.DaughterFilter.PVIPFilterCriterion.MinIPsignif = 3.; // hlt tuned: 3, hlt presel: 1, offline is 4
//
// mass cut
HLTFilterBs2DsH.Window = 600*MeV; // later corrected to 500 MeV downwards
HLTFilterBs2DsH.MinPt = 3000.*MeV; // hlt tuned: 3000 MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2DsH.KillOverlap = true ; 
//
// select B
HLTFilterBs2DsH.MotherFilter.Selections = {"B_s0 : VtxFilterCriterion, 
                                          PVIPFilterCriterion,
                                          FlightDistanceFilterCriterion,
                                          Momentum2FlightAngleFilterCriterion"};

HLTFilterBs2DsH.MotherFilter.VtxFilterCriterion.MaxChi2 = 50; // hlt tuned: 12, hlt presel: 100, offline is 3

//HLTFilterBs2DsH.MotherFilter.KinFilterCriterion.MinPt = 3000.*MeV; // hlt tuned: 3000 MeV, hlt presel: 0 MeV, no offline cut
//HLTFilterBs2DsH.MotherFilter.KinFilterCriterion.MinMomentum = 0.; // hlt tuned: 0 MeV, hlt presel: 0 MeV, no offline cut

HLTFilterBs2DsH.MotherFilter.PVIPFilterCriterion.MaxIPsignif = 4.0 ; // hlt tuned: 8, hlt presel: 1000, offline is 3

HLTFilterBs2DsH.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBs2DsH.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 4.; // hlt tuned: 6, hlt presel: 0, no offline cut

HLTFilterBs2DsH.MotherFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBs2DsH.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.9995; // hlt tuned: 0.9995, hlt presel: 0.0, offline is 8 mrad
//------------------------------------------------------------------------------

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBs2DsH.opts

//==============================================================================
// HLT Selection of B_s0 -> (phi(1020) -> K+ K-) gamma
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBs2PhiGamma.opts


//==============================================================================
// HLT Selection of B_s0 -> (phi(1020) -> K+ K-) gamma
//==============================================================================
// Author: P. Koppenburg,  29/10/2004
// Author: L. Fernandez,   21/11/2004, add gamma
// Author: P. Koppenburg,  04/01/2005, make it a single MakeResonances algorithm

//------------------------------------------------------------------------------
// Filter All Bs -> Phi Gamma
//------------------------------------------------------------------------------
HLTExclusive.Members += {"GaudiSequencer/HLTselBs2PhiGamma"};
HLTselBs2PhiGamma.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselBs2PhiGamma.MeasureTime = true ;

//------------------------------------------------------------------------------
HLTselBs2PhiGamma.Members += {"MakeResonances/HLTFilterBs2PhiGamma"};
HLTFilterBs2PhiGamma.DecayDescriptor = "B_s0 -> phi(1020) gamma";
HLTFilterBs2PhiGamma.PhysDesktop.InputLocations = {"Phys/HLTSharedPhi", "Phys/HLTPreselPhotons"};
// Filter daughters
HLTFilterBs2PhiGamma.DaughterFilter.Selections = { "phi(1020) : VtxFilterCriterion/PhiVtx, 
                                                                MassFilterCriterion/PhiMass, 
								KinFilterCriterion/PhiKin",
                                                   "K+ : KinFilterCriterion/KKin, PVIPFilterCriterion/KPVIP", 
						   "gamma : KinFilterCriterion/GammaKin" } ;
HLTFilterBs2PhiGamma.DaughterFilter.FilterByDescendents = true ;
// kaon cuts
HLTFilterBs2PhiGamma.DaughterFilter.KPVIP.MinIPSignif = 2. ; // offline: 2 sigma
HLTFilterBs2PhiGamma.DaughterFilter.KKin.MinPt = 300.*MeV ; // offline: 0 MeV
// phi cuts
HLTFilterBs2PhiGamma.DaughterFilter.PhiKin.MinPt = 1000.*MeV; // offline = 0
HLTFilterBs2PhiGamma.DaughterFilter.PhiKin.MinMomentum = 0.*MeV; // offline = 0
HLTFilterBs2PhiGamma.DaughterFilter.PhiVtx.MaxChi2 = 100.;  // offline = 49
HLTFilterBs2PhiGamma.DaughterFilter.PhiMass.Window = 15.*MeV ; // offline 10 MeV
// gamma cuts
HLTFilterBs2PhiGamma.DaughterFilter.GammaKin.MinPt = 2800.*MeV; // offline 2.8 GeV
HLTFilterBs2PhiGamma.DaughterFilter.GammaKin.MinMomentum = 0.*MeV;
//
// Combine Phi and Gamma
HLTFilterBs2PhiGamma.Window = 500*MeV; // hlt tuned: xxx MeV, hlt presel: 1000 MeV, offline tight is 200 MeV, loose 1 < GeV
HLTFilterBs2PhiGamma.MinPt = 1000.*MeV;  // hlt tuned: xxx MeV, hlt presel: 0 MeV, no offline cut
HLTFilterBs2PhiGamma.KillOverlap = true ; // unlikely to do anything
//
// Bs cuts
HLTFilterBs2PhiGamma.MotherFilter.Selections = {"B_s0 : VtxFilterCriterion,
                                                         PVIPFilterCriterion,
                                                         Momentum2FlightAngleFilterCriterion"};
HLTFilterBs2PhiGamma.MotherFilter.VtxFilterCriterion.MaxChi2 = 100.; // offline: 49 (does it do anything?)
//HLTFilterBs2PhiGamma.MotherFilter.KinFilterCriterion.MinPt = 1000.*MeV; // hlt tuned: xxx MeV, hlt presel: 0 MeV, no offline cut
//HLTFilterBs2PhiGamma.MotherFilter.KinFilterCriterion.MinMomentum = 0.*MeV; // hlt tuned: xxx MeV, hlt presel: 0 MeV, no offline cut
// HLTFilterBs2PhiGamma.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
// HLTFilterBs2PhiGamma.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 0.; // hlt tuned: xxx, hlt presel: 0, no offline cut
HLTFilterBs2PhiGamma.MotherFilter.PVIPFilterCriterion.MaxIPsignif = 3.0 ; // hlt tuned: xxx, hlt presel: 0, no offline cut
HLTFilterBs2PhiGamma.MotherFilter.Momentum2FlightAngleFilterCriterion.CutBestPV = true;
HLTFilterBs2PhiGamma.MotherFilter.Momentum2FlightAngleFilterCriterion.CosAngle = 0.999; // hlt tuned: xxx, hlt presel: 0.999, offline is 15mrad

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBs2PhiGamma.opts

//==============================================================================
// HLT Selection of B_s0 -> Mu+ Mu-
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselB2MuMu.opts


//==============================================================================
// HLT Selection of B -> Mu Mu
//==============================================================================
// Author: P. Koppenburg,  28/10/2004

//------------------------------------------------------------------------------
// Filter All B -> mumu
//------------------------------------------------------------------------------
HLTExclusive.Members += {"GaudiSequencer/HLTselB2MuMu"};
HLTselB2MuMu.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselB2MuMu.MeasureTime = true ;

//------------------------------------------------------------------------------
// Transform Psi vertex into a Bs0 
HLTselB2MuMu.Members += {"ChangePID/HLTB2MuMu"};   
HLTB2MuMu.PhysDesktop.InputLocations = {"Phys/HltGeneric/JPsi"};
HLTB2MuMu.Particles = { "J/psi(1S)" } ;
HLTB2MuMu.Replacements = { "B_s0"} ;
//------------------------------------------------------------------------------

//------------------------------------------------------------------------------
HLTselB2MuMu.Members += {"FilterDesktop/HLTFilterB2MuMu"} ;
HLTFilterB2MuMu.PhysDesktop.InputLocations = { "Phys/HLTB2MuMu" };
HLTFilterB2MuMu.Filter.Selections = {"B_s0 : MassFilterCriterion
                                         , VtxFilterCriterion
                                         , KinFilterCriterion
                                        ,  PVIPFilterCriterion                                         
                                        ,  FlightDistanceFilterCriterion",                                         
                                   "mu+ : KinFilterCriterion/MuKin,
				          PVIPFilterCriterion/MuPVIP"};
HLTFilterB2MuMu.Filter.FilterByDescendents = true ;

HLTFilterB2MuMu.Filter.MassFilterCriterion.Window = 500*MeV; // offline: 60 MeV
HLTFilterB2MuMu.Filter.VtxFilterCriterion.MaxChi2 = 50.; // offline: 4
HLTFilterB2MuMu.Filter.KinFilterCriterion.MinPt = 3000.*MeV; // offline: 3 GeV
HLTFilterB2MuMu.Filter.KinFilterCriterion.MinMomentum = 0.*MeV; // no offline cut
HLTFilterB2MuMu.Filter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterB2MuMu.Filter.FlightDistanceFilterCriterion.MinFSPV = 6. ; // offline: 29
HLTFilterB2MuMu.Filter.PVIPFilterCriterion.MaxIPsignif = 5.; // no offline cut

HLTFilterB2MuMu.Filter.MuKin.MinPt = 1000.*MeV; // offline: 1.3 GeV
HLTFilterB2MuMu.Filter.MuPVIP.MinIPsignif = 1.; // no offline cut

// <-- End of including file $HLTSELECTIONSROOT/options/HLTselB2MuMu.opts

//==============================================================================
// HLT Selection of B0 -> Mu+ Mu- K*
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselBd2MuMuKstar.opts


//==============================================================================
// HLT Selection of B -> Mu Mu Kstar
//==============================================================================
// Author: P. Koppenburg,  29/10/2004

//------------------------------------------------------------------------------
// Filter All B -> MuMuKstar
//------------------------------------------------------------------------------
HLTExclusive.Members += {"GaudiSequencer/HLTselBd2MuMuKstar"};
HLTselBd2MuMuKstar.IgnoreFilterPassed = false ; // act as real Sequencer
HLTselBd2MuMuKstar.MeasureTime = true ;

//------------------------------------------------------------------------------
// One big algorithm that does everything:
//------------------------------------------------------------------------------
// Combine K* and dimuon
HLTselBd2MuMuKstar.Members += {"MakeResonances/HLTFilterBd2MuMuKstar"};   
HLTFilterBd2MuMuKstar.PhysDesktop.InputLocations = { "Phys/HltGeneric/JPsi", "Phys/HLTSharedKstar"};
HLTFilterBd2MuMuKstar.DecayDescriptor = "[B0 -> J/psi(1S) K*(892)0]cc";
//
// filter daughters using granddaughters too.
HLTFilterBd2MuMuKstar.DaughterFilter.Selections = {"J/psi(1S): VtxFilterCriterion/PsiVtx,
                                                       MassFilterCriterion/PsiMass,
					               FlightDistanceFilterCriterion/PsiFD",
                                            "mu+ : KinFilterCriterion/MuKin",
					    "K*(892)0 : VtxFilterCriterion/KstarVtx,
                                                        MassFilterCriterion/KstarMass,
					                KinFilterCriterion/KstarKin,
					                FlightDistanceFilterCriterion/KstarFD",
					    "K+ : PVIPFilterCriterion/HPVIP",
					    "pi+ : PVIPFilterCriterion/HPVIP, 
					           KinFilterCriterion/PiKin" };
HLTFilterBd2MuMuKstar.DaughterFilter.FilterByDescendents = true ;
// muons from Psi
HLTFilterBd2MuMuKstar.DaughterFilter.MuKin.MinPt = 900*MeV ; // offline: 900 MeV
// Psi
// Offline also rejects Psi and Psi' but we don't want this in the HLT
HLTFilterBd2MuMuKstar.DaughterFilter.PsiVtx.MaxChi2 = 50;  // offline = 8
HLTFilterBd2MuMuKstar.DaughterFilter.PsiMass.UpperWindow = 2100 ; // offline M<5.2GeV
HLTFilterBd2MuMuKstar.DaughterFilter.PsiMass.LowerWindow = 3000 ; // no offline cut
HLTFilterBd2MuMuKstar.DaughterFilter.PsiFD.MinFSPV = 1.0 ; // offline: 1.5
// Filter K and pi
//HLTFilterBd2MuMuKstar.DaughterFilter.HPVIP.MinIPsignif = 2. ; // offline: 2 sigma
HLTFilterBd2MuMuKstar.DaughterFilter.PiKin.MinPt = 200. ; // offline: 200 MeV
// Filter K*
HLTFilterBd2MuMuKstar.DaughterFilter.KstarVtx.MaxChi2 = 30.;  // offline = 8
HLTFilterBd2MuMuKstar.DaughterFilter.KstarMass.Window = 150. ; // offline 100 MeV
HLTFilterBd2MuMuKstar.DaughterFilter.KstarKin.MinPt = 900. ; // offline 900 MeV
HLTFilterBd2MuMuKstar.DaughterFilter.KstarFD.MinFSPV = 0.7 ; // offline: 1.5
//
// combine K* and dimuon
HLTFilterBd2MuMuKstar.Window = 500*MeV; 
HLTFilterBd2MuMuKstar.KillOverlap = true ;
//
// cut on B
HLTFilterBd2MuMuKstar.MotherFilter.Selections = { "B0 : VtxFilterCriterion, 
                                                        PVIPFilterCriterion,
							FlightDistanceFilterCriterion" } ;
HLTFilterBd2MuMuKstar.MotherFilter.FlightDistanceFilterCriterion.CutBestPV = true;
HLTFilterBd2MuMuKstar.MotherFilter.FlightDistanceFilterCriterion.MinFSPV = 6. ; // hlt tuned: 8, hlt presel: 0, offline is 14-20
HLTFilterBd2MuMuKstar.MotherFilter.VtxFilterCriterion.MaxChi2 = 30.; // offline: 10
HLTFilterBd2MuMuKstar.MotherFilter.PVIPFilterCriterion.MaxIPSignif = 5. ; // Offline: 3.5


// <-- End of including file $HLTSELECTIONSROOT/options/HLTselBd2MuMuKstar.opts

//==============================================================================
// HLT Selection of B -> J/Psi(Mu+ Mu-) X
//==============================================================================
// --> Including file $HLTSELECTIONSROOT/options/HLTselB2JpsiX.opts


HLTSelections.Members += {"GaudiSequencer/HLTselB2JpsiX"};
HLTselB2JpsiX.MeasureTime = true ;

//------------------------------------------------------------------------------
// Hot J/Psi Candidates, a subset of the dimuons, for PsiKs
//------------------------------------------------------------------------------
HLTselB2JpsiX.Members += {"FilterDesktop/HLTFilterB2JpsiX"} ;
HLTFilterB2JpsiX.PhysDesktop.InputLocations = { "Phys/HltGeneric/JPsi" };
HLTFilterB2JpsiX.Filter.Selections = { "J/psi(1S) : MassFilterCriterion, 
                                               FlightDistanceFilterCriterion,
					       PVIPFilterCriterion/PsiPVIP,
					       VtxFilterCriterion", 
                                  "mu+ : KinFilterCriterion/MuKin, 
				         PVIPFilterCriterion/MuPVIP"};
// muons
HLTFilterB2JpsiX.Filter.FilterByDescendents = true ;	  
HLTFilterB2JpsiX.Filter.MuKin.MinPt = 500*MeV ; // cut again on Pt
HLTFilterB2JpsiX.Filter.MuPVIP.MinIPsignif = 2.;
// psi
HLTFilterB2JpsiX.Filter.MassFilterCriterion.Window = 50.*MeV ; // offline = 20
HLTFilterB2JpsiX.Filter.FlightDistanceFilterCriterion.MinFSPV = 0.4 ; // offline = 1.2 in z
HLTFilterB2JpsiX.Filter.PsiPVIP.MinIPsignif = 1.0; // offline: 1.4
HLTFilterB2JpsiX.Filter.VtxFilterCriterion.MaxChi2 = 50; // offline = 20


// <-- End of including file $HLTSELECTIONSROOT/options/HLTselB2JpsiX.opts

