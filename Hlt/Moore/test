//==============================================================================
// MC truth
//==============================================================================
// Add an empty sequencer:
HLTSelections.Members += {"GaudiSequencer/TrueSignals"};
TrueSignals.IgnoreFilterPassed = true ;
// You can control the effect of this sequencer from
// $HLTSELCHECKERROT/options/DVHLTCorr.opts
//
//==============================================================================
//==============================================================================
// Prepare particles and intermediate resonances
//==============================================================================
// Filter neutrals
// --> Including file $HLTSELECTIONSROOT/options/HLTNeutrals.opts


HLTSelections.Members += {"GaudiSequencer/HLTNeutrals"};
HLTNeutrals.IgnoreFilterPassed = true ;
HLTNeutrals.MeasureTime = true ;

//------------------------------------------------------------------------------
// Apply some pre-selection cuts on photons
HLTNeutrals.Members += {"FilterDesktop/HLTPreselPhotons"};
HLTPreselPhotons.PhysDesktop.InputLocations = {"Phys/HLTPhotons"};
HLTPreselPhotons.Filter.Selections = {"gamma : KinFilterCriterion"} ;
HLTPreselPhotons.Filter.KinFilterCriterion.MinPt = 300.; // MeV
HLTPreselPhotons.Filter.KinFilterCriterion.MinMomentum = 0.; // MeV

// <-- End of including file $HLTSELECTIONSROOT/options/HLTNeutrals.opts


HLTSelections.Members += {"GaudiSequencer/HLTMakeResonances"};
HLTMakeResonances.IgnoreFilterPassed = true ;
HLTMakeResonances.MeasureTime = true ;
// --> Including file $HLTSELECTIONSROOT/options/HLTFilterParticles.opts



//------------------------------------------------------------------------------
// Detached pions. Also VTT, no Pt cut because of slow pions.
HLTMakeResonances.Members += {"FilterDesktop/HLTDetachedPions"};
HLTDetachedPions.PhysDesktop.InputLocations = {"Phys/HLTPions"};
HLTDetachedPions.Filter.Selections = {"pi+ : PVIPFilterCriterion"} ;
HLTDetachedPions.Filter.PVIPFilterCriterion.MinIPsignif = 2.; // sigma
HLTDetachedPions.Filter.ExclusiveSelection = true ;
//------------------------------------------------------------------------------
// Apply some pre-selection cuts on pions
HLTMakeResonances.Members += {"FilterDesktop/HLTPreselPions"};
HLTPreselPions.PhysDesktop.InputLocations = {"Phys/HLTDetachedPions"};
HLTPreselPions.Filter.Selections = {"pi+ : TrackTypeFilterCriterion, KinFilterCriterion"} ;
HLTPreselPions.Filter.KinFilterCriterion.MinPt = 300.; // MeV
HLTPreselPions.Filter.KinFilterCriterion.MinMomentum = 0.; // MeV
HLTPreselPions.Filter.TrackTypeFilterCriterion.RequireLong = true ; // only long
HLTPreselPions.Filter.ExclusiveSelection = true ;
//------------------------------------------------------------------------------
// Apply some pre-selection cuts on kaons
HLTMakeResonances.Members += {"FilterDesktop/HLTPreselKaons"};
HLTPreselKaons.PhysDesktop.InputLocations = {"Phys/HLTKaons"};
HLTPreselKaons.Filter.Selections = {"K+ : TrackTypeFilterCriterion, 
                                          KinFilterCriterion,
                                          PVIPFilterCriterion"} ;
HLTPreselKaons.Filter.KinFilterCriterion.MinPt = 300.; // MeV
HLTPreselKaons.Filter.KinFilterCriterion.MinMomentum = 0.; // MeV
HLTPreselKaons.Filter.PVIPFilterCriterion.MinIPsignif = 2.; // sigma
HLTPreselKaons.Filter.TrackTypeFilterCriterion.RequireLong = true ; // only long
HLTPreselKaons.Filter.ExclusiveSelection = true ;

// <-- End of including file $HLTSELECTIONSROOT/options/HLTFilterParticles.opts

// --> Including file $HLTSELECTIONSROOT/options/HLTSharedKstar.opts


//------------------------------------------------------------------------------
// Form 2-prong seeding vertices
// The name must be the same as in TransformSeedingVertices.opts + "Mother"
HLTMakeResonances.Members += {"MakeResonances/HLTSharedKstar"} ;
HLTSharedKstar.PhysDesktop.InputLocations = {"Phys/HLTPreselPions", "Phys/HLTPreselKaons"};
HLTSharedKstar.DecayDescriptor = "[K*(892)0 -> K+ pi-]cc" ;
HLTSharedKstar.Window = 120*MeV ; //
HLTSharedKstar.MotherFilter.Selections = {  "K*(892)0 : VtxFilterCriterion"} ;
//                                                     ,    KinFilterCriterion 
//							, PVIPFilterCriterion"} ;
HLTSharedKstar.MotherFilter.VtxFilterCriterion.MaxChi2 = 200; // hlt tuned: 200, hlt presel: 200 
//HLTSharedKstar.MotherFilter.KinFilterCriterion.MinPt = 0.; // MeV
//HLTSharedKstar.MotherFilter.KinFilterCriterion.MinMomentum = 0.; // MeV
//HLTSharedKstar.MotherFilter.PVIPFilterCriterion.MinIPsignif = 0.; // sigma

// <-- End of including file $HLTSELECTIONSROOT/options/HLTSharedKstar.opts

// --> Including file $HLTSELECTIONSROOT/options/HLTSharedPhi.opts


