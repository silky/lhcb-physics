<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension//en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
  <argument name="program"><text>../options/Moore-TestIndependence.py</text></argument>
  <argument name="timeout">
    <integer>7200</integer>
  </argument>
  <argument name="use_temp_dir"><enumeral>true</enumeral></argument>
  <argument name="args"><set>
    <text>-n</text>
    <text>10000</text>
    <text>-s</text>
    <text>--dddbtag=head-20110302</text>
    <text>--conddbtag=head-20110622</text>
    <text>--hlt1lines=all</text>
    <text>--hlt2lines=all</text>
    <text>--l0</text>
    <text>--nprocesses=8</text>
    <text>Physics_Sept2011</text>
  </set></argument>
  <argument name="unsupported_platforms"><set><text>win.*</text></set></argument>
  <argument name="validator"><text>
expected_string = &quot;No mismatches found.&quot;
if stdout.find(expected_string) == -1:
    causes.append('missing string')
    result['GaudiTest.expected_string'] = result.Quote(expected_string)
</text></argument>

</extension>
