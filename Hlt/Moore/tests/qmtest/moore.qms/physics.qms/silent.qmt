<?xml version="1.0" ?><!DOCTYPE extension  PUBLIC '-//QM/2.3/Extension//EN'  'http://www.codesourcery.com/qm/dtds/2.3/-//qm/2.3/extension//en.dtd'>
<extension class="GaudiTest.GaudiExeTest" kind="test">
  <argument name="program"><text>gaudirun.py</text></argument>
  <argument name="args"><set>
    <text>../options/Moore_Hlt1And2.py</text>
    <text>--option</text>
    <text>from Configurables import Moore; Moore().Verbose=False; Moore().Silent=True; Moore().EvtMax=100; </text>
  </set></argument>
  <argument name="unsupported_platforms"><set><text>win.*</text></set></argument>
  <argument name="validator"><text>

if len(stdout.split('\n'))&gt;85:
    causes.append('stdout is too long, aiming for less than 85 lines')
    result['moore.physics.silent']=result.Quote('Expected less than 85 lines, got '+ str(len(stdout.split('\n'))))

if len(stdout)&gt;20000:
    causes.append('stdout is too long, aiming for less than 20k characters')
    result['moore.physics.silent']=result.Quote('Expected less than 20k characters, got '+ str(len(stdout)))

#count error lines, shouldn't be any warnings or errors, ideally
#error lines here are stupid Gaudi warnings about not being able to navigate to input file, 3 input files means 6 errors.
countErrorLines({"FATAL":0,"ERROR":2,"WARNING":1})

</text></argument>
</extension>

