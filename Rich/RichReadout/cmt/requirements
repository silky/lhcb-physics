package RichReadout
version v1r9

# ================ structure =================================================
branches cmt doc src
include_path none

# ================ environment ===============================================
set RICHREADOUTOPTS $(RICHREADOUTROOT)/options

# ================ dependencies ==============================================
use RichEvent        v7r*    Event

# ================ component library =========================================
library RichReadout ../src/*.cpp  -import=AIDA

apply_pattern component_library library=RichReadout
