package  GiGaCnv 
version  v8r0

# ============== package structure ============================================
branches         cmt src doc GiGaCnv 

# ============== explicit dependencies ========================================
use DetDesc      v11r*	      Det
        
use HepMC        v3r121p0     Sim
use GenEvent     v2r*         Event

use GiGa         v10r*        Sim 
use Event        v3r*	      Event

# ============== standard includes ============================================
include_dirs     $(GIGACNVROOT)

# ============== linker library (base classes) ================================
library          GiGaCnvLib            Lib/*.cpp

# ============== component library ============================================
library          GiGaCnv               component/*.cpp 	        

# ============== standard patterns ============================================
apply_pattern    component_library library=GiGaCnv 
apply_pattern    linker_library    library=GiGaCnvLib 

