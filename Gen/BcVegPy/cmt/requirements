package       BcVegPy
version       v2r0

# =========== structure =======================================================
branches      cmt src include doc

# =========== own includes ====================================================
include_dirs $(BCVEGPYROOT)/include

# =========== dependencies ====================================================
use      GENSER            v*     Gen
use      BcVegPyData       v*

# =========== constituents ====================================================
library BcVegPy   *.F

# =========== standard patterns ===============================================
apply_pattern linker_library  library=BcVegPy
apply_pattern linker_stamps   library=BcVegPy

