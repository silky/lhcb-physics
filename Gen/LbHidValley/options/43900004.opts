// include "common" configuration file:
// ============================================================================
#include "$LBHIDVALLEYOPTS/HidValleyH.opts"
// ============================================================================
// define the event type 
// ============================================================================
Generation.EventType = 43900004 ;
// ============================================================================
// define the particular properties of H_20 and H_30 
// ============================================================================
ParticlePropertySvc.Particles = {
  "H_20 88 35 0.0 180.0 9.4e-26   Higgs'0 35 0.0e+00" ,
  "H_30 89 36 0.0  35.0 100.0e-12      A0 36 0.0e+00"
} ;
