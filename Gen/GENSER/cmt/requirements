package GENSER
version v2r49p0

branches doc cmt src

use LCG_Settings v*
use GaudiPolicy  v*

include_path none

macro pythia6_native_version "408.2"
use pythia6  v* LCG_GeneratorsInterfaces
macro_remove pythia6_linkopts " -lpythia6_pdfdummy "

macro photos_native_version "215.2"
use photos v* LCG_GeneratorsInterfaces

macro lhapdf_native_version "5.2.3"
use lhapdf v* LCG_GeneratorsInterfaces

library GENSER  ../src/*.f
apply_pattern package_Flinkopts

macro_append GENSER_linkopts "$(pythia6_linkopts) $(lhapdf_linkopts) $(photos_linkopts) -lnsl -lcrypt -ldl -lg2c"
