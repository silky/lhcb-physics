package GENSER
version v1r030p0

branches doc cmt

use LCG_Settings v*

include_path none

macro GENSER_native_version "GENSER_0_3_0"
macro GENSER_home "$(LCG_releases)/GENSER/$(GENSER_native_version)/$(LCG_system)"

path_remove LD_LIBRARY_PATH "/GENSER/"
path_append LD_LIBRARY_PATH "$(GENSER_home)/lib"

